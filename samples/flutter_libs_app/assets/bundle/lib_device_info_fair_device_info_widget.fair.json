{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "leading": {
          "className": "GestureDetector",
          "na": {
            "child": {
              "className": "Icon",
              "pa": [
                "#(Icons.arrow_back)"
              ],
              "na": {
                "size": 20
              }
            },
            "onTap": "@(_pop)"
          }
        },
        "centerTitle": true,
        "title": {
          "className": "Text",
          "pa": [
            "#(widget.title)"
          ]
        }
      }
    },
    "body": {
      "className": "Row",
      "na": {
        "mainAxisSize": "#(MainAxisSize.max)",
        "mainAxisAlignment": "#(MainAxisAlignment.center)",
        "children": [
          {
            "className": "Column",
            "na": {
              "crossAxisAlignment": "#(CrossAxisAlignment.center)",
              "mainAxisSize": "#(MainAxisSize.max)",
              "mainAxisAlignment": "#(MainAxisAlignment.center)",
              "children": [
                {
                  "className": "Text",
                  "pa": [
                    "#($_deviceInfo)"
                  ],
                  "na": {
                    "style": {
                      "className": "TextStyle",
                      "na": {
                        "fontSize": 16,
                        "color": {
                          "className": "Color",
                          "pa": [
                            "0xFF333333"
                          ]
                        }
                      }
                    }
                  }
                },
                {
                  "className": "Padding",
                  "na": {
                    "padding": {
                      "className": "EdgeInsets.only",
                      "na": {
                        "top": 20
                      }
                    },
                    "child": {
                      "className": "GestureDetector",
                      "na": {
                        "child": {
                          "className": "Container",
                          "na": {
                            "width": 300,
                            "padding": {
                              "className": "EdgeInsets.symmetric",
                              "na": {
                                "vertical": 10
                              }
                            },
                            "decoration": {
                              "className": "BoxDecoration",
                              "na": {
                                "borderRadius": {
                                  "className": "BorderRadius.circular",
                                  "pa": [
                                    10
                                  ]
                                },
                                "color": {
                                  "className": "Color",
                                  "pa": [
                                    "0xFF1882F1"
                                  ]
                                }
                              }
                            },
                            "child": {
                              "className": "Text",
                              "pa": [
                                "获取设备信息"
                              ],
                              "na": {
                                "textAlign": "#(TextAlign.center)",
                                "style": {
                                  "className": "TextStyle",
                                  "na": {
                                    "fontSize": 16,
                                    "color": "#(Colors.white)"
                                  }
                                }
                              }
                            }
                          }
                        },
                        "onTap": "@(_getDeviceInfo)"
                      }
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  },
  "methodMap": {
    "createState": "%(_FairDeviceInfoWidgetState)"
  }
}