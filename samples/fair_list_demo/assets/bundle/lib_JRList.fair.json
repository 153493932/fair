{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "Pull Refresh"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "RefreshIndicator",
          "na": {
            "displacement": 10.0,
            "child": {
              "className": "ListView.builder",
              "na": {
                "controller": "#(_scrollController)",
                "itemCount": "#(list.length)",
                "itemBuilder": "#(itemBuilder)"
              }
            },
            "onRefresh": "#(_refresh)"
          }
        }
      }
    }
  }
}